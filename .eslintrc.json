{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["google"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "require-jsdoc": "off",
    "linebreak-style": "off",
    "max-len": "off",
    "new-cap": "off",
    "no-unused-vars": "warn"
  }
}
